    <!-- ========== UNIT ECONOMICS ========== -->
    <section class="py-24">
      <div class="max-w-4xl mx-auto px-6">
        <div class="text-center mb-4">
          <h2 class="text-3xl sm:text-4xl font-bold font-heading mb-3">Transparent Economics</h2>
          <p class="text-text-secondary">We keep costs low so you get a premium AI without the premium price.</p>
        </div>

        <div class="mt-12">
          <svg viewBox="0 0 700 320" class="w-full" xmlns="http://www.w3.org/2000/svg">
            <!-- Background -->
            <rect width="700" height="320" fill="transparent"/>

            <!-- Grid lines -->
            <line x1="100" y1="40" x2="100" y2="240" stroke="#1f2937" stroke-width="1"/>
            <line x1="100" y1="240" x2="620" y2="240" stroke="#1f2937" stroke-width="1"/>
            <line x1="100" y1="140" x2="620" y2="140" stroke="#1f2937" stroke-width="0.5" stroke-dasharray="4"/>
            <line x1="100" y1="90" x2="620" y2="90" stroke="#1f2937" stroke-width="0.5" stroke-dasharray="4"/>
            <line x1="100" y1="190" x2="620" y2="190" stroke="#1f2937" stroke-width="0.5" stroke-dasharray="4"/>

            <!-- Y-axis labels -->
            <text x="90" y="244" fill="#64748b" font-size="11" text-anchor="end" font-family="Inter, sans-serif">$0</text>
            <text x="90" y="194" fill="#64748b" font-size="11" text-anchor="end" font-family="Inter, sans-serif">$25</text>
            <text x="90" y="144" fill="#64748b" font-size="11" text-anchor="end" font-family="Inter, sans-serif">$50</text>
            <text x="90" y="94" fill="#64748b" font-size="11" text-anchor="end" font-family="Inter, sans-serif">$75</text>
            <text x="90" y="44" fill="#64748b" font-size="11" text-anchor="end" font-family="Inter, sans-serif">$100</text>

            <!-- Starter Group -->
            <text x="220" y="270" fill="#94a3b8" font-size="13" text-anchor="middle" font-family="Space Grotesk, sans-serif" font-weight="600">Starter $19/mo</text>
            <!-- Infrastructure -->
            <rect x="170" y="224" rx="3" ry="3" width="30" height="16" fill="#ef4444" opacity="0.8"/>
            <!-- API cost -->
            <rect x="205" y="220" rx="3" ry="3" width="30" height="20" fill="#f59e0b" opacity="0.8"/>
            <!-- Margin -->
            <rect x="240" y="220" rx="3" ry="3" width="30" height="20" fill="#22c55e" opacity="0.8"/>
            <!-- Labels on bars -->
            <text x="185" y="218" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$4</text>
            <text x="220" y="214" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$5</text>
            <text x="255" y="214" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$10</text>
            <text x="220" y="295" fill="#64748b" font-size="10" text-anchor="middle" font-family="Inter, sans-serif">53% margin</text>

            <!-- Pro Group -->
            <text x="390" y="270" fill="#94a3b8" font-size="13" text-anchor="middle" font-family="Space Grotesk, sans-serif" font-weight="600">Pro $49/mo</text>
            <!-- Infrastructure -->
            <rect x="340" y="216" rx="3" ry="3" width="30" height="24" fill="#ef4444" opacity="0.8"/>
            <!-- API cost -->
            <rect x="375" y="200" rx="3" ry="3" width="30" height="40" fill="#f59e0b" opacity="0.8"/>
            <!-- Margin -->
            <rect x="410" y="194" rx="3" ry="3" width="30" height="46" fill="#22c55e" opacity="0.8"/>
            <!-- Labels on bars -->
            <text x="355" y="210" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$6</text>
            <text x="390" y="194" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$20</text>
            <text x="425" y="188" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$23</text>
            <text x="390" y="295" fill="#64748b" font-size="10" text-anchor="middle" font-family="Inter, sans-serif">47% margin</text>

            <!-- Power Group -->
            <text x="530" y="270" fill="#94a3b8" font-size="13" text-anchor="middle" font-family="Space Grotesk, sans-serif" font-weight="600">Power $99/mo</text>
            <!-- Infrastructure -->
            <rect x="480" y="216" rx="3" ry="3" width="30" height="24" fill="#ef4444" opacity="0.8"/>
            <!-- API cost -->
            <rect x="515" y="160" rx="3" ry="3" width="30" height="80" fill="#f59e0b" opacity="0.8"/>
            <!-- Margin -->
            <rect x="550" y="134" rx="3" ry="3" width="30" height="106" fill="#22c55e" opacity="0.8"/>
            <!-- Labels on bars -->
            <text x="495" y="210" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$6</text>
            <text x="530" y="154" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$40</text>
            <text x="565" y="128" fill="#94a3b8" font-size="9" text-anchor="middle" font-family="Inter, sans-serif">$53</text>
            <text x="530" y="295" fill="#64748b" font-size="10" text-anchor="middle" font-family="Inter, sans-serif">54% margin</text>

            <!-- Legend -->
            <rect x="180" y="308" width="10" height="10" rx="2" fill="#ef4444" opacity="0.8"/>
            <text x="195" y="317" fill="#94a3b8" font-size="10" font-family="Inter, sans-serif">Infrastructure</text>
            <rect x="290" y="308" width="10" height="10" rx="2" fill="#f59e0b" opacity="0.8"/>
            <text x="305" y="317" fill="#94a3b8" font-size="10" font-family="Inter, sans-serif">API Cost</text>
            <rect x="380" y="308" width="10" height="10" rx="2" fill="#22c55e" opacity="0.8"/>
            <text x="395" y="317" fill="#94a3b8" font-size="10" font-family="Inter, sans-serif">Your Margin</text>
          </svg>
        </div>
      </div>
